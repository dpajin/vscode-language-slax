{
  "name": "juniper-slax",
  "displayName": "Juniper SLAX",
  "description": "Syntax highlighting for SLAX",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.5.0"
  },
  "icon": "images/space.png",
  "publisher": "dpajin",
  "repository": {
    "type": "git",
    "url": "https://github.com/dpajin/vscode-language-slax.git"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "slax",
        "aliases": [
          "SLAX",
          "slax"
        ],
        "extensions": [
          ".slax"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "slax",
        "scopeName": "source.slax",
        "path": "./syntaxes/Slax.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "slax",
        "path": "./snippets/snippets.json"
      }
    ]
  }
}
